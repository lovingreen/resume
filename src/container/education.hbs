<main>
    <section>
        {{>title data='My education' big=true}}
        {{#with main}}
        <ul>
            {{#each educations}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (Ще в процесі) {{/unless}}
                </li>
            {{/each}}
        </ul>

    </section>
    <section>
        {{>title data='My certificates'}}
        <ul>
            {{#each certificates}}
                <li>
                    {{name}}
                    ({{id}})
                </li>
            {{/each}}
        </ul>
    </section>
    {{/with}}
</main>